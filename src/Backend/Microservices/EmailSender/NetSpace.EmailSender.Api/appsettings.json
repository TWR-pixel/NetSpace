{
    "AllowedHosts": "*",
    "EmailSenderOptions": {
        "From": "from",
        "Password": "pass",
        "Host": "smtp.yandex.ru",
        "Port": 465
    },
    "Serilog": {
        "WriteTo": [
            {
                "Args": {
                    "restrictedToMinimumLevel": "Information"
                },
                "Name": "Console"
            },

            {
                "Args": {
                    "path": "logs/log.log",
                    "rollingInterval": "Day"
                },
                "Name": "File"
            }
        ]
    }
}
